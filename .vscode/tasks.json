{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compilar Hash Project",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-Wall",
                "-Wextra",
                "-std=c11",
                "src/main.c",
                "src/hashtable.c",
                "src/dlist.c",
                "src/sort_utils.c",
                "src/utils.c",
                "-o",
                "build/hash_app.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        }
    ]
}
